<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">腳本包</system:String>
    <system:String x:Key="miscTab">其他</system:String>
    <system:String x:Key="settingsTab">腳本包設置</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">腳本包路徑:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="openFolder">打開資料夾</system:String>
    <system:String x:Key="reloadList">重新讀取清單</system:String>
    <system:String x:Key="reloadScript">重新讀取腳本包</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">第一個音符</system:String>
    <system:String x:Key="lastNote">最後一個音符</system:String>
    <system:String x:Key="noteScreenTime">音符速度</system:String>

    <system:String x:Key="profile">輪廓</system:String>
    <system:String x:Key="save">儲存</system:String>
    <system:String x:Key="delete">刪除</system:String>
    <system:String x:Key="defaults">預設</system:String>

</ResourceDictionary>