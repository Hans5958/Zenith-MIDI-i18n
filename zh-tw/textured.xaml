<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Palette picker-->
    <system:String x:Key="palettes_reload">重新讀取</system:String>
    <system:String x:Key="palettes_imageNote">圖片至少需要16(或32位元)像素寬png檔案, 和任何高度.支援透明.</system:String>
    <system:String x:Key="palettes_randomise">隨機顏色順序</system:String>

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">材質包</system:String>
    <system:String x:Key="miscTab">其它</system:String>
	<system:String x:Key="switchesTab">切換</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">材質包路徑:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="reloadList">重新讀取清單</system:String>
    <system:String x:Key="reloadPack">重新讀取材質包</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">第一個音符</system:String>
    <system:String x:Key="lastNote">最後一個音符</system:String>
    <system:String x:Key="pianoHeight">鋼琴高度 %</system:String>
    <system:String x:Key="noteScreenTime">音符速度</system:String>
    <system:String x:Key="blackNotesAbove">上面畫黑色音符 (警告: 很慢!!)</system:String>
</ResourceDictionary>