<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="alignments">位置</system:String>
    <system:String x:Key="alTopLeft">左上角</system:String>
    <system:String x:Key="alTopRight">右上角</system:String>
    <system:String x:Key="alBottomLeft">左下角</system:String>
    <system:String x:Key="alBottomRight">右下角</system:String>
    <system:String x:Key="alTopSpread">上面伸展</system:String>
    <system:String x:Key="alBottomSpread">下面伸展</system:String>

    <system:String x:Key="fontSize">字體大小</system:String>
    <system:String x:Key="font">字體</system:String>

    <system:String x:Key="reload">重新讀取</system:String>
    <system:String x:Key="openFolder">打開資料夾</system:String>

    <system:String x:Key="savePath">儲存位置</system:String>
    <system:String x:Key="browse">瀏覽</system:String>
    <system:String x:Key="saveToFile">儲存.csv資料</system:String>
    <system:String x:Key="formatString">CSV 格式</system:String>
</ResourceDictionary>