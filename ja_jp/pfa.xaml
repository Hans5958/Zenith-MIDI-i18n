<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Other settings-->
    <system:String x:Key="firstNote">最初のノート</system:String>
    <system:String x:Key="lastNote">最後のノート</system:String>
    <system:String x:Key="pianoHeight">鍵盤の高さ (%)</system:String>
    <system:String x:Key="noteScreenTime">ノート速度(速←→遅)</system:String>
    <system:String x:Key="sameWidthNotes">全ノート同じ幅</system:String>
    <system:String x:Key="save">設定を保存</system:String>
    <system:String x:Key="loadSaved">保存した設定</system:String>
    <system:String x:Key="setDefaults">デフォルト設定</system:String>

    <system:String x:Key="middleCSquare">中央のドの鍵に四角形</system:String>
    <system:String x:Key="blackNotesAbove">黒鍵ノートを白鍵ノートのあとにレンダー (重いよ!)</system:String>
    <system:String x:Key="topColor">鍵盤の上</system:String>
    <system:String x:Key="colorRed">赤</system:String>
    <system:String x:Key="colorGreen">緑</system:String>
    <system:String x:Key="colorBlue">青</system:String>

    <system:String x:Key="borderWidth">枠の太さ</system:String>
</ResourceDictionary>