<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Palette picker-->
    <system:String x:Key="palettes_reload">リロード</system:String>
    <system:String x:Key="palettes_imageNote">画像は、幅16px(グラデーション使用は32px)と任意の高さのPNGでなくてはなりません。透明使えます。</system:String>
    <system:String x:Key="palettes_randomise">色の順番をシャッフル</system:String>

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">リソース</system:String>
    <system:String x:Key="miscTab">その他</system:String>
    <system:String x:Key="switchesTab">機能切り替え</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">リソースパックパス:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="reloadList">パックリストをリロード</system:String>
    <system:String x:Key="reloadPack">パックをリロード</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">最初のノート</system:String>
    <system:String x:Key="lastNote">最後のノート</system:String>
    <system:String x:Key="pianoHeight">鍵盤の高さ (%)</system:String>
    <system:String x:Key="noteScreenTime">ノート速度(速←→遅)</system:String>
    <system:String x:Key="blackNotesAbove">黒鍵ノートを白鍵ノートのあとにレンダー (重いよ!)</system:String>
</ResourceDictionary>