<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">Resources</system:String>
    <system:String x:Key="miscTab">Misc</system:String>
    <system:String x:Key="switchesTab">Switches</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">Paczki zasobów:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="openFolder">Otwórz Folder</system:String>
    <system:String x:Key="reloadList">Załaduj ponownie Listę</system:String>
    <system:String x:Key="reloadPack">Reload Pack</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">Pierwsza nuta</system:String>
    <system:String x:Key="lastNote">Ostatnia Nuta</system:String>
    <system:String x:Key="noteScreenTime">Note screen time</system:String>
    <system:String x:Key="blackNotesAbove">Rysuj czarne nuty (WOLNIEJSZE!)</system:String>
</ResourceDictionary>