    <system:String x:Key="cameraPreset">Kiểu góc nhìn</system:String>
    <system:String x:Key="farPreset">Xa</system:String>
    <system:String x:Key="mediumPreset">Bình thường</system:String>
    <system:String x:Key="closerPreset">Closer</system:String>
    <system:String x:Key="closePreset">Gần hơn</system:String>

