    <system:String x:Key="reload">Làm mới</system:String>
    <system:String x:Key="auraStrength">Bán kính ánh hào quang</system:String>
    <system:String x:Key="auraLocation">Ảnh được sử dụng ở: Plugins\Assets\MIDITrail\Aura</system:String>
