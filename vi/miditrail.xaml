<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Palette picker-->
    <system:String x:Key="palettes_reload">Làm mới</system:String>
    <system:String x:Key="palettes_imageNote">Bảng màu cần phải có 16 đến 32 điểm ảnh ngang, không tính điểm ảnh dọc.Hỗ trợ độ trong suốt.</system:String>
    <system:String x:Key="palettes_randomise">Ngẫu nhiên hóa bảng màu</system:String>

    <!--Shared-->
    <system:String x:Key="delete">Xóa</system:String>
    <system:String x:Key="saveNew">Lưu mới</system:String>
    <system:String x:Key="loadDefault">Đặt lại về mặc định</system:String>

    <!--Tab Names-->
    <system:String x:Key="visualsTab">Hiển thị</system:String>
    <system:String x:Key="cameraTab">Camera</system:String>
    <system:String x:Key="auraTab">Ánh hào quang</system:String>


    <!--Visuals-->
    <system:String x:Key="firstNote">Phím đầu</system:String>
    <system:String x:Key="lastNote">Phím cuối</system:String>
    <system:String x:Key="keyDownSpeed">Tốc độ ấn phím</system:String>
