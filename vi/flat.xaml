<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Palette picker-->
    <system:String x:Key="palettes_reload">Làm mới</system:String>
    <system:String x:Key="palettes_imageNote">Bảng màu cần phải có 16 đến 32 điểm ảnh ngang, không tính điểm ảnh dọc.Hỗ trợ độ trong suốt.</system:String>
    <system:String x:Key="palettes_randomise">Ngẫu nhiên hóa bảng màu</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">Phím đầu</system:String>
    <system:String x:Key="lastNote">Phím cuối</system:String>
    <system:String x:Key="pianoHeight">Chiều cao bàn phím (tính bằng %)</system:String>
    <system:String x:Key="noteScreenTime">Tốc độ nốt nhạc</system:String>
    <system:String x:Key="sameWidthNotes">Các nốt nhạc có cùng chiều rộng</system:String>
    <system:String x:Key="save">Lưu</system:String>
    <system:String x:Key="loadSaved">Tải bản đã lưu</system:String>
    <system:String x:Key="setDefaults">Đặt lại về mặc định</system:String>

</ResourceDictionary>