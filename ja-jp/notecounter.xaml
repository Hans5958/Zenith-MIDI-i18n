    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="alignments">配置</system:String>
    <system:String x:Key="alTopLeft">左上</system:String>
    <system:String x:Key="alTopRight">右上</system:String>
    <system:String x:Key="alBottomLeft">左下</system:String>
    <system:String x:Key="alBottomRight">右下</system:String>
