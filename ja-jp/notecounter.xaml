<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="alignments">配置</system:String>
    <system:String x:Key="alTopLeft">左上</system:String>
    <system:String x:Key="alTopRight">右上</system:String>
    <system:String x:Key="alBottomLeft">左下</system:String>
    <system:String x:Key="alBottomRight">右下</system:String>
    <system:String x:Key="alTopSpread">上 (横に広げる)</system:String>
    <system:String x:Key="alBottomSpread">下 (横に広げる)</system:String>

    <system:String x:Key="fontSize">フォントサイズ</system:String>
    <system:String x:Key="font">フォント</system:String>

    <system:String x:Key="reload">リロード</system:String>
    <system:String x:Key="openFolder">フォルダを開く</system:String>

    <system:String x:Key="thousandsSeparator">1000の区切り</system:String>
    <system:String x:Key="commas">コンマ</system:String>
    <system:String x:Key="nothing">なし</system:String>

    <system:String x:Key="savePath">保存先</system:String>
    <system:String x:Key="browse">参照</system:String>
    <system:String x:Key="saveToFile">フレームごとの情報を.csvファイルに保存</system:String>
    <system:String x:Key="formatString">CSV行フォーマット</system:String>
