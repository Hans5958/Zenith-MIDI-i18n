<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">リソース</system:String>
    <system:String x:Key="miscTab">その他</system:String>
    <system:String x:Key="settingsTab">スクリプト設定</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">リソースパックパス:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="openFolder">フォルダを開く</system:String>
    <system:String x:Key="reloadList">リストをリロード</system:String>
    <system:String x:Key="reloadScript">スクリプトをリロード</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">最初のノート</system:String>
    <system:String x:Key="lastNote">最後のノート</system:String>
    <system:String x:Key="noteScreenTime">ノート速度(速←→遅)</system:String>

    <system:String x:Key="profile">プロファイル</system:String>
    <system:String x:Key="save">設定を保存</system:String>
    <system:String x:Key="loadSaved">保存した設定</system:String>
    <system:String x:Key="setDefaults">デフォルト設定</system:String>

</ResourceDictionary>