    <system:String x:Key="forceRender">Forzar re-render en cada cuadro</system:String>
	
    <!--Render Page-->
    <system:String x:Key="savePath">Direccion de guardado</system:String>
    <system:String x:Key="includeAudio">Incluir Audio</system:String>
    <system:String x:Key="renderMask">Mascara de transparencia de render</system:String>
    <system:String x:Key="bitrate">Bitrate</system:String>
    <system:String x:Key="crfHint">FFMPEG crf: 0 = lossless, 51 = Peor calidad posible</system:String>
    <system:String x:Key="delatStart">Delay de inicio(segundos)</system:String>
    <system:String x:Key="ffmpegDebug">ffmpeg debug</system:String>
    <system:String x:Key="startRender">Iniciar Render</system:String>
    <system:String x:Key="ifUploading">Si sube el video en linea, por favor dar credito al programa.</system:String>
	
    <!--NEW-->
    <system:String x:Key="realtimePlayback">Reproduccion en tiempo real</system:String>
    <system:String x:Key="useOmniMidi">OmniMIDI Activado (Mas uso de memoria)</system:String>
    <system:String x:Key="muteAudio">Silenciar audio</system:String>
    <system:String x:Key="disableKDMAPI">Inhabilitar KDMAPI</system:String>
    <system:String x:Key="enableKDMAPI">Habilitar KDMAPI</system:String>
    <system:String x:Key="settingsTab">Configuracion</system:String>
    <system:String x:Key="noteSize">Tamaño de nota</system:String>
    <system:String x:Key="tickBased">Basado en tick</system:String>
    <system:String x:Key="timeBased">Basado en tiempo</system:String>
    <system:String x:Key="ignoreColorEvents">Ignorar eventos de color</system:String>
    <system:String x:Key="useBackgroundImage">Usar imagen de fondo</system:String>
	
	<!--NEW 1.6-->
    <system:String x:Key="pauseHintNew">Presiona la barra espaciadora para pausar/play, Presiona enter en la ventana de la vista previa para ver en pantalla completa</system:String>
