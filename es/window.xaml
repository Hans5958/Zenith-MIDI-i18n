    <system:String x:Key="superRes">SSAA</system:String>
    <system:String x:Key="superResWarning">Incrementando el SSAA Puede mejorar la calidad de los pixeles, pero requiere mejor desempeño</system:String>
    <system:String x:Key="language">Idioma</system:String>
    
    <!--Plugins Page-->
    <system:String x:Key="reload">Recargar</system:String>
    <system:String x:Key="pluginWarning">AVISO: Solo descarga plugins de fuentes conocidas. Es posible esconder un virus dentro de un plugin</system:String>
    <system:String x:Key="startPreview">Iniciar Vista previa</system:String>
    <system:String x:Key="stop">Detener</system:String>

    <!--Preview Page-->
    <system:String x:Key="vsync">V-Sync Habilitado</system:String>
    <system:String x:Key="tempo">Multiplicador de Tempo</system:String>
