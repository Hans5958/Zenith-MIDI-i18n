<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="LanguageName">Español</system:String>

    <!--Tab Names-->
    <system:String x:Key="midiTab">Cargar MIDI</system:String>
    <system:String x:Key="pluginsTab">Complementos</system:String>
    <system:String x:Key="pluginSettingsTab">Configuracion de Plugin</system:String>
    <system:String x:Key="previewTab">Muestra</system:String>
    <system:String x:Key="renderTab">Render</system:String>
    
    <!--Main Page-->
    <system:String x:Key="load">Cargar</system:String>
    <system:String x:Key="unload">Descargar</system:String>
    <system:String x:Key="browse">Buscar</system:String>
    <system:String x:Key="maxTrackBuffSize">Tamaño máximo del buffer de pista (en bytes)</system:String>
    <system:String x:Key="width">Ancho</system:String>
    <system:String x:Key="height">Alto</system:String>
    <system:String x:Key="fps">FPS</system:String>
    <system:String x:Key="presets">Presets</system:String>
