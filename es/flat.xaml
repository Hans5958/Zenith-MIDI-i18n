<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Other settings-->
    <system:String x:Key="firstNote">Primera Nota</system:String>
    <system:String x:Key="lastNote">Ultima Nota</system:String>
    <system:String x:Key="pianoHeight">Altura del piano (%)</system:String>
    <system:String x:Key="noteScreenTime">Note screen time</system:String>
    <system:String x:Key="sameWidthNotes">Mismo ancho de notas</system:String>
    <system:String x:Key="save">Guardar</system:String>
    <system:String x:Key="loadSaved">Cargar guardado</system:String>
    <system:String x:Key="setDefaults">Restaurar configuraciones predeterminadas</system:String>

</ResourceDictionary>