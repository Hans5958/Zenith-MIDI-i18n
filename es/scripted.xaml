<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">Recursos</system:String>
    <system:String x:Key="miscTab">Miscelaneos</system:String>
    <system:String x:Key="settingsTab">Configuracion del script</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">Ubicacion de los paquetes de recursos:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="openFolder">Abrir carpeta</system:String>
    <system:String x:Key="reloadList">Recargar lista</system:String>
    <system:String x:Key="reloadScript">Recargar script</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">Primera nota</system:String>
    <system:String x:Key="lastNote">Ultima nota</system:String>
    <system:String x:Key="noteScreenTime">Tiempo de notas en pantalla</system:String>

    <system:String x:Key="profile">Perfil</system:String>
    <system:String x:Key="save">Guardar</system:String>
    <system:String x:Key="delete">Borrar</system:String>
    <system:String x:Key="defaults">Configuraciones predeterminadas</system:String>

</ResourceDictionary>