<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">资源</system:String>
    <system:String x:Key="miscTab">杂项</system:String>
    <system:String x:Key="switchesTab">开关</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">资源包路径:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="openFolder">打开文件夹</system:String>
    <system:String x:Key="reloadList">重新加载列表</system:String>
    <system:String x:Key="reloadPack">重新加载资源包</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">第一个音符</system:String>
    <system:String x:Key="lastNote">最后一个音符</system:String>
    <system:String x:Key="noteScreenTime">音符速度</system:String>
    <system:String x:Key="blackNotesAbove">显示黑色的音符轮廓(渲染得更慢)</system:String>
</ResourceDictionary>