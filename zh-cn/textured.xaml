<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Palette picker-->
    <system:String x:Key="palettes_reload">重新加载</system:String>
    <system:String x:Key="palettes_imageNote">图像要求是宽度为16或32像素、高度为无限制的PNG文件。支持透明文件。</system:String>
    <system:String x:Key="palettes_randomise">随机颜色顺序</system:String>

    <!--Tab Names-->
    <system:String x:Key="resourcesTab">资源</system:String>
    <system:String x:Key="miscTab">杂项</system:String>
    <system:String x:Key="switchesTab">开关</system:String>

    <!--Resources-->
    <system:String x:Key="resourcePath">资源包路径:</system:String>
    <system:String x:Key="resourcePathDir">Plugins\Assets\Textured\Resources</system:String>
    <system:String x:Key="reloadList">重新加载列表</system:String>
    <system:String x:Key="reloadPack">重新加载包</system:String>

    <!--Other settings-->
    <system:String x:Key="firstNote">第一个音符</system:String>
    <system:String x:Key="lastNote">最后一个音符</system:String>
    <system:String x:Key="pianoHeight">钢琴高度</system:String>
    <system:String x:Key="noteScreenTime">音符在屏幕上的时间</system:String>
    <system:String x:Key="blackNotesAbove">显示黑色的音符轮廓（渲染得更慢）</system:String>
</ResourceDictionary>