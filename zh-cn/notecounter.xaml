<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="alignments">位置</system:String>
    <system:String x:Key="alTopLeft">左上角</system:String>
    <system:String x:Key="alTopRight">右上角</system:String>
    <system:String x:Key="alBottomLeft">左下角</system:String>
    <system:String x:Key="alBottomRight">右下角</system:String>
    <system:String x:Key="alTopSpread">顶部(横向)</system:String>
    <system:String x:Key="alBottomSpread">底部(横向)</system:String>

    <system:String x:Key="fontSize">字体大小</system:String>
    <system:String x:Key="font">字体</system:String>

    <system:String x:Key="reload">重新加载</system:String>
    <system:String x:Key="openFolder">打开文件夹</system:String>

    <system:String x:Key="savePath">保存位置</system:String>
    <system:String x:Key="browse">打开</system:String>
    <system:String x:Key="saveToFile">保存帧文件到CSV文件</system:String>
    <system:String x:Key="formatString">CSV格式</system:String>
</ResourceDictionary>