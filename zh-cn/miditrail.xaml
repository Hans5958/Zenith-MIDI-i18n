<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Shared-->
    <system:String x:Key="delete">删除</system:String>
    <system:String x:Key="saveNew">保存新的配置</system:String>
    <system:String x:Key="loadDefault">默认</system:String>

    <!--Tab Names-->
    <system:String x:Key="visualsTab">视觉效果</system:String>
    <system:String x:Key="cameraTab">相机位置</system:String>
    <system:String x:Key="auraTab">光晕</system:String>


    <!--Visuals-->
    <system:String x:Key="firstNote">第一个音符</system:String>
    <system:String x:Key="lastNote">最后一个音符</system:String>
    <system:String x:Key="keyDownSpeed">钢琴键按下速度</system:String>
    <system:String x:Key="keyUpSpeed">钢琴键上弹速度</system:String>
    <system:String x:Key="tiltKeys">倾斜钢琴键(效果未知)</system:String>
    <system:String x:Key="3dNotes">立体音符(渲染更慢)</system:String>
    <system:String x:Key="lightShade">更浅的颜色</system:String>
    <system:String x:Key="noteSpeed">音符速度</system:String>
    <system:String x:Key="velocityStrength">音符速度的强度(效果未知)</system:String>
    <system:String x:Key="keyboardClip">不在钢琴键后方显示音符</system:String>
    <system:String x:Key="sameWidthNotes">统一音符宽度</system:String>
    <system:String x:Key="onNoteHit">当一个音符按下时:</system:String>
    <system:String x:Key="changeSize">改变音符大小</system:String>
    <system:String x:Key="changeTint">改变色调</system:String>
    <system:String x:Key="showKeyboard">显示键盘</system:String>


    <!--Camera-->
    <system:String x:Key="speedWarning">警告：此选项会使用更多的RAM和CPU。同屏的音符越多，渲染速度越慢！</system:String>

    <system:String x:Key="cameraPreset">预置:</system:String>
    <system:String x:Key="farPreset">远</system:String>
    <system:String x:Key="mediumPreset">中等</system:String>
    <system:String x:Key="closerPreset">更近</system:String>
    <system:String x:Key="closePreset">关闭</system:String>

    <system:String x:Key="FOV">视角大小(FOV)</system:String>
    <system:String x:Key="viewAngle">倾斜角度</system:String>
    <system:String x:Key="renderDistF">前方音符区域大小</system:String>
    <system:String x:Key="renderDistB">后方音符区域大小</system:String>
    <system:String x:Key="camOffsets">相机偏移</system:String>
    <system:String x:Key="horizontal">水平</system:String>
    <system:String x:Key="vertical">垂直</system:String>

    <system:String x:Key="viewTilt">相机倾斜</system:String>
    <system:String x:Key="viewTurn">相机转角</system:String>
	<system:String x:Key="viewSpin">相机旋转</system:String>
    <system:String x:Key="offsetX">X</system:String>
    <system:String x:Key="offsetY">Y</system:String>
    <system:String x:Key="offsetZ">Z</system:String>
    <system:String x:Key="topPreset">顶部</system:String>
    <system:String x:Key="perspectivePreset">视角</system:String> 

    <!--Aura-->
    <system:String x:Key="imageHint">图像只能是方形png文件，并且只包含透明度</system:String>
    <system:String x:Key="auraEnabled">开启光晕</system:String>
    <system:String x:Key="reload">重新加载</system:String>
    <system:String x:Key="auraStrength">渐变强度</system:String>
    <system:String x:Key="openAuraFolder">打开光晕文件夹</system:String>
</ResourceDictionary>